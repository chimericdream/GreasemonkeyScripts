{
    "name": "@chimericdream/auto-download-instructables",
    "nice-name": "Auto-Download Instructables",
    "version": "1.1.1",
    "description": "Greasemonkey script to automatically navigate to the download page for an Instructable and trigger the download.",
    "gm-build-info": {
        "grant": ["none"],
        "require": [
            "https://raw.githubusercontent.com/chimericdream/JS-URL-Helpers/master/qsvar.js",
            "https://raw.githubusercontent.com/chimericdream/JS-URL-Helpers/master/absurl.js"
        ],
        "urls": [
            "http://instructables.com/id/*",
            "https://instructables.com/id/*",
            "http://www.instructables.com/id/*",
            "https://www.instructables.com/id/*"
        ]
    }
}